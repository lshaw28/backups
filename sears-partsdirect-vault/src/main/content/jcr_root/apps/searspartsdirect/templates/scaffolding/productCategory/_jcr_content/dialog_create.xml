<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <tab1
            jcr:primaryType="cq:Panel"
            title="Properties">
            <items jcr:primaryType="cq:WidgetCollection">
            	<trueName
            		jcr:primaryType="cq:Widget"
                    fieldLabel="Category True Name"
                    allowBlank="false"
                    blankText="True name is required"
                    regex="/^[a-zA-Z0-9_-]*$/"
                    regexText="True name may only contain letters, digits, hyphens, or underscores"
                    maxLength="40"
                    name=":name"/>
                <title
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Title"
                    allowBlank="false"
                    name="./jcr:content/jcr:title"/>
                <titlePlural
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Title Plural Form"
                    allowBlank="false"
                    name="./jcr:content/titlePlural"/>
                <description
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Description"
                    xtype="textarea"
                    name="./jcr:content/jcr:description"/>
                <articleIndexCopy
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Article Index Copy"
                    fieldLabelDescription="Description for Article Index page"
                    name="./jcr:content/articleIndexCopy"
                    xtype="textarea"/>
            </items>
        </tab1>
        <icons
			jcr:primaryType="cq:Panel"
			title="Icons">
			<items jcr:primaryType="cq:WidgetCollection">
		        <subcontainer2
					jcr:primaryType="cq:Widget"
		                  flex="{Long}1"
		                  height="250"
		                  xtype="dialogfieldset">
		        	<items jcr:primaryType="cq:WidgetCollection">
			        	<iconImages
				            jcr:primaryType="nt:unstructured"
				            fieldLabel="Choose Icon(s)"
				            name="./jcr:content/iconImages"
				            type="select"
				            xtype="selection">
				            <options jcr:primaryType="cq:WidgetCollection"
		                   		path="/apps/searspartsdirect/components/page/base/list_page_icons.infinity.json"
		                    	xtype="cqinclude"/>
						</iconImages>
					</items>
		        </subcontainer2>
			</items>
		</icons>
        <tab3
        	jcr:primaryType="cq:Panel"
        	title="Category 101">
            <items jcr:primaryType="cq:WidgetCollection">
	        	<title jcr:primaryType="cq:Widget"
					   fieldLabel="New Header"
					   name="./jcr:content/header"
					   xtype="textfield"
					   maxLength="42"
					   fieldDescription="Character limit is 42" />
	        	<multi
		            jcr:primaryType="cq:Widget"
		            fieldLabel="Articles"
		            name="./jcr:content/multipaths"
		            xtype="multifieldlimit"
		            limit="6" >
		            <fieldConfig
		                jcr:primaryType="cq:Widget"
		                xtype="multicustom">
		                <items jcr:primaryType="cq:WidgetCollection">
		                    <url
		                        jcr:primaryType="cq:Widget"
		                        fieldLabel="Article"
		                        name="url"
		                        rootPath="/content/searspartsdirect/en/article"
		                        xtype="pathfield"/>
		                </items>
		            </fieldConfig>
		    	</multi>
		    	<viewall
				    	jcr:primaryType="cq:Widget"
			            fieldLabel="Enter link text"
			            fieldDescription="Enter Text to display link"
			            name="./jcr:content/viewAllLinkText"
			            xtype="textfield"/>
			     <includeCommonParts
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Include Common Parts"
                    name="./jcr:content/includeCommonParts"
                    type="checkbox"
                    xtype="selection"/>

                 <includeMaintenanceTips
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Include Maintenance Tips"
                    name="./jcr:content/includeMaintenanceTips"
                    type="checkbox"
                    xtype="selection"/>

                 <includeCommonQuestions
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Include Common Questions"
                    name="./jcr:content/includeCommonQuestions"
                    type="checkbox"
                    xtype="selection"/>
	    	</items>
	    </tab3>
        <tab4
        	jcr:primaryType="cq:Panel"
        	title="Related Assets">
        	<items jcr:primaryType="cq:WidgetCollection">
        		<tags
        			jcr:primaryType="cq:Widget"
        			name="./jcr:content/cq:tags"
        			fieldLabel="Tag Associations"
        			xtype="tags"/>
        		<pages
        			jcr:primaryType="cq:Widget"
        			name="./jcr:content/pages"
					treeRootPath="/etc/spdAssets/scaffolding/accessory"
					multipleRootPaths="[accessory]"
        			hideLabel="{Boolean}false"
        			fieldLabel="Page Associations"
        			xtype="scaffoldingpagemapper"/>
        	</items>
       	</tab4>
        <tab5
        	jcr:primaryType="cq:Panel"
        	title="SkyScraper Ad"/>
       	<desktop
       		jcr:primaryType="cq:Panel"
       		title="160x600 Image">
            <items jcr:primaryType="cq:WidgetCollection">
		       	<desktopImageTab
					jcr:primaryType="cq:Widget"
					itemId="desktopImageTab"
					cropParameter="./jcr:content/desktopImage/imageCrop"
					ddGroups="[media]"
					fileNameParameter="./jcr:content/desktopImage/fileName"
					fileReferenceParameter="./jcr:content/desktopImage/fileReference"
					blankText="An image is required"
					name="./jcr:content/desktopImage/file"
					requestSuffix="/jcr:content/desktopImage.img.png"
					rotateParameter="./jcr:content/desktopImage/imageRotate"
					sizeLimit="100"
					uploadUrl="/tmp/upload/*"
		            height="400"
					xtype="html5smartimage" />
			</items>
		</desktop>
       	<tablet
       		jcr:primaryType="cq:Panel"
       		title="768x90 Image">
            <items jcr:primaryType="cq:WidgetCollection">
				<tabletImageTab
					jcr:primaryType="cq:Widget"
					itemId="tabletImageTab"
					cropParameter="./jcr:content/tabletImage/imageCrop"
					ddGroups="[media]"
					fileNameParameter="./jcr:content/tabletImage/fileName"
					fileReferenceParameter="./jcr:content/tabletImage/fileReference"
					blankText="An image is required"
					name="./jcr:content/tabletImage/file"
					requestSuffix="/jcr:content/tabletImage.img.png"
					rotateParameter="./jcr:content/tabletImage/imageRotate"
					sizeLimit="100"
					uploadUrl="/tmp/upload/*"
		            height="400"
					xtype="html5smartimage" />
			</items>
		</tablet>
       	<mobile
       		jcr:primaryType="cq:Panel"
       		title="300x50 Image">
            <items jcr:primaryType="cq:WidgetCollection">
				<mobileImageTab
					jcr:primaryType="cq:Widget"
					itemId="mobileImageTab"
					cropParameter="./jcr:content/mobileImage/imageCrop"
					ddGroups="[media]"
					fileNameParameter="./jcr:content/mobileImage/fileName"
					fileReferenceParameter="./jcr:content/mobileImage/fileReference"
					blankText="An image is required"
					name="./jcr:content/mobileImage/file"
					requestSuffix="/jcr:content/mobileImage.img.png"
					rotateParameter="./jcr:content/mobileImage/imageRotate"
					sizeLimit="100"
					uploadUrl="/tmp/upload/*"
		            height="400"
					xtype="html5smartimage" />
			</items>
		</mobile>
		<square
       		jcr:primaryType="cq:Panel"
       		title="300x250 Image">
            <items jcr:primaryType="cq:WidgetCollection">
				<squareImageTab
					jcr:primaryType="cq:Widget"
					itemId="squareImageTab"
					cropParameter="./jcr:content/squareImage/imageCrop"
					ddGroups="[media]"
					fileNameParameter="./jcr:content/squareImage/fileName"
					fileReferenceParameter="./jcr:content/squareImage/fileReference"
					blankText="An image is required"
					name="./jcr:content/squareImage/file"
					requestSuffix="/jcr:content/squareImage.img.png"
					rotateParameter="./jcr:content/squareImage/imageRotate"
					sizeLimit="100"
					uploadUrl="/tmp/upload/*"
		            height="400"
					xtype="html5smartimage" />
			</items>
		</square>
       	<linkTab
			jcr:primaryType="cq:Widget"
			title="Image Link"
			xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">
				<linkAlt
					jcr:primaryType="cq:Widget"
					fieldLabel="Alternative Text"
					allowBlank="{Boolean}true"
					itemId="linkAlt"
					name="./jcr:content/linkAlt"
					xtype="textfield" />
				<linkURL
					jcr:primaryType="cq:Widget"
					fieldLabel="Link"
					allowBlank="{Boolean}true"
					itemId="linkURL"
					name="./jcr:content/linkURL"
					xtype="pathfield" />
				<linkTarget
					jcr:primaryType="cq:Widget"
					fieldLabel="Target"
					name="./jcr:content/linkTarget"
					type="select"
					xtype="selection">
					<options
						jcr:primaryType="cq:WidgetCollection">
						<r1
							jcr:primaryType="nt:unstructured"
							text="New Window"
							value="_blank" />
						<r2
							jcr:primaryType="nt:unstructured"
							text="This Window"
							value="_self" />
					</options>
				</linkTarget>
			</items>
		</linkTab>
		<imageDetails
			jcr:primaryType="cq:Widget"
			title="Image Details"
			xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">
				<displayWidth
                    jcr:primaryType="cq:Widget"
                    fieldDescription="Set a specific display width for the image."
                    fieldLabel="Display Width"
                    name="./jcr:content/displayWidth"
                    xtype="textfield"/>
				<displayHeight
                    jcr:primaryType="cq:Widget"
                    fieldDescription="Set a specific display height for the image."
                    fieldLabel="Display Height"
                    name="./jcr:content/displayHeight"
                    xtype="textfield"/>
				<desktopResType
					jcr:primaryType="cq:Widget"
					ignoreData="{Boolean}true"
					name="./jcr:content/desktopImage/sling:resourceType"
					value="foundation/components/image"
					xtype="hidden" />
				<tabletResType
					jcr:primaryType="cq:Widget"
					ignoreData="{Boolean}true"
					name="./jcr:content/tabletImage/sling:resourceType"
					value="foundation/components/image"
					xtype="hidden" />
				<mobileResType
					jcr:primaryType="cq:Widget"
					ignoreData="{Boolean}true"
					name="./jcr:content/mobileImage/sling:resourceType"
					value="foundation/components/image"
					xtype="hidden" />
				<squareResType
					jcr:primaryType="cq:Widget"
					ignoreData="{Boolean}true"
					name="./jcr:content/squareImage/sling:resourceType"
					value="foundation/components/image"
					xtype="hidden" />
				<caption jcr:primaryType="cq:Widget"
					fieldLabel="Caption"
					name="./jcr:content/imageCaption"
					xtype="textfield" />
				<credit jcr:primaryType="cq:Widget"
					fieldLabel="Photo Credit"
					name="./jcr:content/photoCredit"
					xtype="textfield" />
			</items>
		</imageDetails>
    </items>
</jcr:root>
