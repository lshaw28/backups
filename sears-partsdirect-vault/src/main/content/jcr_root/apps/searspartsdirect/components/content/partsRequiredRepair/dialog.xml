<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
		  jcr:primaryType="cq:Dialog"
		  title="Parts Required for Repair"
		  xtype="dialog">
	<items jcr:primaryType="cq:WidgetCollection">
		<Panel 	jcr:primaryType="cq:Widget"
						xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">
			
				<title jcr:primaryType="cq:Widget"
					   fieldLabel="Parts Required Title"
					   fieldDescription="Character limit is 50"
					   allowBlank="{Boolean}false"
					   name="./partsRequiredTitle"
					   xtype="textfield"
					   maxLength="50" />
					   
				<list  jcr:primaryType="cq:Widget"
					   fieldLabel="Parts Required List"
					   name="./partsRequired"
					   xtype="textfield" />
					   
			</items>
		</Panel>
	</items>
</jcr:root>