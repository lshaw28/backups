<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
	jcr:primaryType="cq:Dialog"
	xtype="dialog"
	title="Glossary">
	<items jcr:primaryType="cq:WidgetCollection">
		<Panel jcr:primaryType="cq:Widget"
			xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">	
				<multi jcr:primaryType="cq:Widget"
		            fieldLabel="Add/Edit Part Type Glossary"
		            name="./Glossary"
		            xtype="multifield">
		            <fieldConfig jcr:primaryType="cq:Widget"
		                xtype="multicustom">
		                	<items jcr:primaryType="cq:WidgetCollection">
			                     <code jcr:primaryType="cq:Widget"
			                        fieldLabel="Glossary letters"
			                        name="glossaryLetter"
			                        xtype="textfield"/>
			                </items>
		            </fieldConfig>
		        </multi>
	       </items>
	    </Panel>
      </items> 
</jcr:root>