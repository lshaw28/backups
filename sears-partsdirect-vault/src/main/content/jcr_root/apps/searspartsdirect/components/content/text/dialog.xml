<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
	jcr:primaryType="cq:TabPanel"
	activeTab="0"
	xtype="tabpanel">
	<items jcr:primaryType="cq:WidgetCollection">
	<tab1 jcr:primaryType="cq:Widget" title="Type"
			xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">
				<type jcr:primaryType="cq:Widget" fieldLabel="Component Type" defaultValue="default"
					name="./componentType" type="select" xtype="selection">
					<options jcr:primaryType="cq:WidgetCollection">
						<one
            				jcr:primaryType="nt:unstructured"
            				text="Default Text Type"
            				value="default"/> 
            			<two
            				jcr:primaryType="nt:unstructured"
            				text="SEO Text Component"
            				value="seoTextComponent"/>
            			<three
            				jcr:primaryType="nt:unstructured"
            				text="Value Prop Component"
            				value="valueprop"/>
            		</options>
            	</type>
            				
			</items>
		</tab1>
		<tab2
			jcr:primaryType="cq:Widget"
			title="Text"
			xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">
				<ContentTiyle jcr:primaryType="cq:Widget" allowBlank="true" fieldLabel="Content Title" itemId="text1" name="./title1" xtype="textfield" />
				<text
					jcr:primaryType="cq:Widget"
					fieldLabel="Text"
					externalStyleSheets="[/etc/designs/searspartsdirect/clientlib_base/less/components/text/generated/text.css]"
					name="./text"
					xtype="richtext">
					<rtePlugins
						jcr:primaryType="nt:unstructured"
						path="/apps/searspartsdirect/components/content/text/limitedConfig.infinity.json"
						xtype="cqinclude"/>
				</text>
				<richFlag
					jcr:primaryType="cq:Widget"
					ignoreData="{Boolean}true"
					name="./textIsRich"
					value="true"
					xtype="hidden"/>
			</items>
		</tab2>
	</items>
</jcr:root>