<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
		  jcr:primaryType="cq:Dialog"
		  title="GenericHeader"
		  xtype="dialog">
	<items jcr:primaryType="cq:WidgetCollection">
		<GenericHeader 	jcr:primaryType="cq:Widget"
						xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">
				<textvalue jcr:primaryType="cq:Widget"
					   fieldLabel="Text Value"
					   fieldDescription="Character limit is 140"
					   allowBlank="{Boolean}false"
					   name="./textvalue"
					   xtype="textfield"
					   maxLength="140" />
			</items>
		</GenericHeader>
	</items>
</jcr:root>