<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
		  jcr:primaryType="cq:Dialog"
		  title="Maintenance Tip Topic"
		  xtype="dialog">
	<items jcr:primaryType="cq:WidgetCollection">
		<MaintenanceTipTopic 	jcr:primaryType="cq:Widget"
						xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">
				<topicTitle jcr:primaryType="cq:Widget"
					   fieldLabel="Topic Title"
					   fieldDescription="Character limit is 50"
					   allowBlank="{Boolean}false"
					   name="./topicTitle"
					   xtype="textfield"
					   maxLength="50" />
			</items>
		</MaintenanceTipTopic>
	</items>
</jcr:root>