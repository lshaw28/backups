<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
		jcr:primaryType="cq:Dialog"
		padding="0"
		title="Carousel"
		height="400"
		width="750"
		xtype="dialog">
	<items jcr:primaryType="cq:Widget"
		    xtype="tabpanel">
	    <items jcr:primaryType="cq:WidgetCollection">
			<tab1 jcr:primaryType="cq:Panel"
					border="false"
					height=""
					title="Carousel Items"
					width="">
				<items jcr:primaryType="cq:WidgetCollection">
					<title jcr:primaryType="cq:Widget"
							fieldDescription="Carousel title"
							fieldLabel="Title"
							name="./titleText"
							xtype="textfield">
					</title>
					<primary jcr:primaryType="cq:Widget"
							fieldDescription="Click the '+' button to add a new image"
							fieldLabel="Image names"
							name="./primary"
							xtype="multifield">
						<fieldConfig jcr:primaryType="cq:Widget"
						        xtype="spd.carouselMultifield" />
					</primary>
				</items>
			</tab1>
	    </items>
	</items>
</jcr:root>