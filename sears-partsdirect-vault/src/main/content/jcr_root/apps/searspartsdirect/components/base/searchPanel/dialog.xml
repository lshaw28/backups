<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
		jcr:primaryType="cq:TabPanel"
		title="Search Panel"
		xtype="dialog">
	<items jcr:primaryType="cq:WidgetCollection">
		<tabpanel jcr:primaryType="cq:TabPanel">
			<items jcr:primaryType="cq:WidgetCollection">
				<tab1   jcr:primaryType="cq:Widget"
					    title="Find Manuals"
					    xtype="panel">
					    <items jcr:primaryType="cq:WidgetCollection">
							<header jcr:primaryType="cq:Widget" 
							       fieldLabel="Header" 
							       name="./header1" 
							       xtype="textfield" />
							<subheader jcr:primaryType="cq:Widget" 
							       fieldLabel="Subheader" 
							       name="./subheader1" 
							       xtype="textfield" />
							<icon
					            jcr:primaryType="nt:unstructured"
					            fieldLabel="Choose Icon"
					            name="./panelIcon1"
					            type="select"
					            xtype="selection">
					            <options jcr:primaryType="cq:WidgetCollection">
					                <iconOne
					                    jcr:primaryType="nt:unstructured"
					                    text="First Icon"
					                    value="iconONe"/>
					                <iconTwo
					                    jcr:primaryType="nt:unstructured"
					                    text="Second Icon"
					                    value="iconTwo"/>
					                <iconThree
					                    jcr:primaryType="nt:unstructured"
					                    text="Third Icon"
					                    value="iconThree"/>
					                <iconFour
					                    jcr:primaryType="nt:unstructured"
					                    text="Fourth Icon"
					                    value="iconFour"/>
					                <iconFive
					                    jcr:primaryType="nt:unstructured"
					                    text="Fifth Icon"
					                    value="iconFive"/>
					            </options>
							</icon>   
					    </items>
				 </tab1>
				 <tab2   jcr:primaryType="cq:Widget"
					    title="Repair and Care Tips"
					    xtype="panel">
					    <items jcr:primaryType="cq:WidgetCollection">
							<header jcr:primaryType="cq:Widget" 
							       fieldLabel="Header" 
							       name="./header2" 
							       xtype="textfield" />
							<subheader jcr:primaryType="cq:Widget" 
							       fieldLabel="Subheader" 
							       name="./subheader2" 
							       xtype="textfield" />
							<icon
					            jcr:primaryType="nt:unstructured"
					            fieldLabel="Choose Icon"
					            name="./panelIcon2"
					            type="select"
					            xtype="selection">
					            <options jcr:primaryType="cq:WidgetCollection">
					                <iconOne
					                    jcr:primaryType="nt:unstructured"
					                    text="First Icon"
					                    value="iconONe"/>
					                <iconTwo
					                    jcr:primaryType="nt:unstructured"
					                    text="Second Icon"
					                    value="iconTwo"/>
					                <iconThree
					                    jcr:primaryType="nt:unstructured"
					                    text="Third Icon"
					                    value="iconThree"/>
					                <iconFour
					                    jcr:primaryType="nt:unstructured"
					                    text="Fourth Icon"
					                    value="iconFour"/>
					                <iconFive
					                    jcr:primaryType="nt:unstructured"
					                    text="Fifth Icon"
					                    value="iconFive"/>
					            </options>
							</icon>   
					    </items>
				</tab2>
				<tab3   jcr:primaryType="cq:Widget"
					    title="Have a Question"
					    xtype="panel">
					    <items jcr:primaryType="cq:WidgetCollection">
							<header jcr:primaryType="cq:Widget" 
							       fieldLabel="Header" 
							       name="./header3" 
							       xtype="textfield" />
							<subheader jcr:primaryType="cq:Widget" 
							       fieldLabel="Subheader" 
							       name="./subheader3" 
							       xtype="textfield" />
							<icon
					            jcr:primaryType="nt:unstructured"
					            fieldLabel="Choose Icon"
					            name="./panelIcon3"
					            type="select"
					            xtype="selection">
					            <options jcr:primaryType="cq:WidgetCollection">
					                <iconOne
					                    jcr:primaryType="nt:unstructured"
					                    text="First Icon"
					                    value="iconONe"/>
					                <iconTwo
					                    jcr:primaryType="nt:unstructured"
					                    text="Second Icon"
					                    value="iconTwo"/>
					                <iconThree
					                    jcr:primaryType="nt:unstructured"
					                    text="Third Icon"
					                    value="iconThree"/>
					                <iconFour
					                    jcr:primaryType="nt:unstructured"
					                    text="Fourth Icon"
					                    value="iconFour"/>
					                <iconFive
					                    jcr:primaryType="nt:unstructured"
					                    text="Fifth Icon"
					                    value="iconFive"/>
					            </options>
							</icon>   
					    </items>
				</tab3>
			</items>
		</tabpanel>
	</items>
</jcr:root>
