<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
		jcr:primaryType="cq:Dialog"
		title="Search Panel"
		xtype="tabpanel">
	<items jcr:primaryType="cq:WidgetCollection">
		<tab1 jcr:primaryType="cq:Widget"
			title="Item 1"
			xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">
				<header jcr:primaryType="cq:Widget"
					fieldLabel="Header"
					name="./header1"
					xtype="textfield" />
				<subheader jcr:primaryType="cq:Widget"
					fieldLabel="Subheader"
					name="./subheader1"
					xtype="textfield" />
				<url jcr:primaryType="cq:Widget"
					fieldLabel="Link"
					name="./url1"
					regex="Shc.components.extsrc.regex.ValidationPatterns.url"
					fieldDescription="Must be a valid link"
					xtype="textfield"/>
				<url1Target
					jcr:primaryType="cq:Widget"
					fieldLabel="Target"
					name="./url1Target"
					type="select"
					xtype="selection">
					<options
						jcr:primaryType="cq:WidgetCollection">
						<r1
							jcr:primaryType="nt:unstructured"
							text="New Window"
							value="_blank" />
						<r2
							jcr:primaryType="nt:unstructured"
							text="This Window"
							value="_self" />
					</options>
				</url1Target>
				<icon
					jcr:primaryType="nt:unstructured"
					fieldLabel="Choose Icon"
					name="./panelIcon1"
					type="select"
					xtype="selection">
					<options jcr:primaryType="cq:WidgetCollection">
						<iconOne
							jcr:primaryType="nt:unstructured"
							text="File"
							value="icon-file-alt"/>
						<iconTwo
							jcr:primaryType="nt:unstructured"
							text="Bell"
							value="icon-bell"/>
						<iconThree
							jcr:primaryType="nt:unstructured"
							text="Envelope"
							value="icon-envelope"/>
						<iconFour
							jcr:primaryType="nt:unstructured"
							text="Home"
							value="icon-home"/>
						<iconFive
							jcr:primaryType="nt:unstructured"
							text="Heart"
							value="icon-heart-empty"/>
					</options>
				</icon>
			</items>
		 </tab1>
		 <tab2 jcr:primaryType="cq:Widget"
			title="Item 2"
			xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">
