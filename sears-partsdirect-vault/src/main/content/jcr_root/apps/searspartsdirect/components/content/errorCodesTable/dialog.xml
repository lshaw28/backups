<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
	jcr:primaryType="cq:Panel"
	activeTab="0"
	xtype="panel">
	<items jcr:primaryType="cq:WidgetCollection">
		<multi
            jcr:primaryType="cq:Widget"
            fieldLabel="Error Code Table"
            name="./errorCodeTable"
            xtype="multifield">
            <fieldConfig
                jcr:primaryType="cq:Widget"
                xtype="multicustom">
                <items jcr:primaryType="cq:WidgetCollection">
                    <codeType
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Error Code Type"
                        name="codeType"
                        xtype="textfield"/>
                     <code
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Error Code"
                        name="code"
                        xtype="textfield"/>
                     <condition
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Condition"
                        name="condition"
                        xtype="textfield"/>
                      <checkRepairLink
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Check/Repair"
                        name="checkRepairLink"
                        xtype="pathfield"/>          
                </items>
            </fieldConfig>
        </multi>
	</items>
</jcr:root>