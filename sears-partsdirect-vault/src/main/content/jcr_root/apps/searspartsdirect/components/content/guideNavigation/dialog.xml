<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" 
	xmlns:jcr="http://www.jcp.org/jcr/1.0" 
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="cq:Dialog"
	title="Guide Navigation"
	height="400"
	width="750"
	xtype="dialog">
	<items
		jcr:primaryType="cq:Widget"
		xtype="tabpanel">
		<items jcr:primaryType="cq:WidgetCollection">
			<tab1 jcr:primaryType="cq:Panel"
				border="false"
				title="Sections">
				<items jcr:primaryType="cq:WidgetCollection">
					<primary jcr:primaryType="cq:Widget"
						fieldDescription="Click the '+' button to add a new section"
						fieldLabel="Sections"
						name="./sections"
						xtype="multifield">
						<fieldConfig jcr:primaryType="cq:Widget"
						             xtype="spd.sectionMultifield" />
					</primary>
				</items>
			</tab1>
	    </items>
	</items>
</jcr:root>