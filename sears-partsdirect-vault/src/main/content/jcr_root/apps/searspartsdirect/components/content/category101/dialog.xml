<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
		  jcr:primaryType="cq:Dialog"
		  title="Category 101"
		  xtype="dialog">
	<items jcr:primaryType="cq:WidgetCollection">
		<Panel 	jcr:primaryType="cq:Widget"
				xtype="panel">
			<items jcr:primaryType="cq:WidgetCollection">
				<title jcr:primaryType="cq:Widget"
					   fieldLabel="New Header"
					   name="./header"
					   xtype="textfield"
					   maxLength="42"
					   fieldDescription="Character limit is 42" />
					   
				<multi
			            jcr:primaryType="cq:Widget"
			            fieldLabel="Articles"
			            name="./multipaths"
			            xtype="multifieldlimit"
			            limit="6" >
			            <fieldConfig
			                jcr:primaryType="cq:Widget"
			                xtype="multicustom">
			                <items jcr:primaryType="cq:WidgetCollection">
			                    <url
			                        jcr:primaryType="cq:Widget"
			                        fieldLabel="Article"
			                        name="url"
			                        rootPath="/content/searspartsdirect/en/articles"
			                        xtype="pathfield"/>
			                </items>
			            </fieldConfig>
			    </multi>
			    
			    <viewall
				    	jcr:primaryType="cq:Widget"
			            fieldLabel="Enter link text"
			            fieldDescription="Enter Text to display link"
			            name="./viewAllLinkText"
			            xtype="textfield"/>	
			</items>
		</Panel>
	</items>
</jcr:root>