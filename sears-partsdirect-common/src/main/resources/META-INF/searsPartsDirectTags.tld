			<name>categoryName</name>
			<type>java.lang.String</type>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>requiredTools</name>
		<tag-class>com.spd.cq.searspartsdirect.common.tags.RequiredToolsTag</tag-class>
		<info>Sets page context information required for Tools Required for Repair</info>
	</tag>
