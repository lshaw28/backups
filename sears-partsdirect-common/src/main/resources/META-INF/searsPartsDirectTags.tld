		<name>getAssets</name>
		<tag-class>com.spd.cq.searspartsdirect.common.tags.GetAssetsTag</tag-class>
 		<info>Returns a list of SPD Assets for given filters</info>
 		<attribute>
			<name>assetType</name>
			<type>java.lang.String</type>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
 		<attribute>
			<name>brandFilter</name>
			<type>java.lang.String</type>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
 		<attribute>
			<name>productCategoryFilter</name>
			<type>java.lang.String</type>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
 		<attribute>
			<name>tagFilter</name>
			<type>java.lang.String</type>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
