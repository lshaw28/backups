	   	<attribute>
			<name>categoryPath</name>
			<type>java.lang.String</type>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>		
	</tag>
	<tag>
		<name>getRelatedArticles</name>
		<tag-class>com.spd.cq.searspartsdirect.common.tags.GetRelatedArticlesTag</tag-class>
		<info>Sets page context information required for Related Articles component</info>
		<attribute>
			<name>categoryPath</name>
			<type>java.lang.String</type>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>	
	<tag>
	   <name>getBrandCategoryModelNo</name>
	   <tag-class>com.spd.cq.searspartsdirect.common.tags.GetBrandCategoryModelNoTag</tag-class>
		 <info>Provides the Brand Category and Model No.</info>
	</tag>
	<tag>
		<name>getModelSymptoms</name>
		<tag-class>com.spd.cq.searspartsdirect.common.tags.ModelSymptomsTag</tag-class>
	   	<info>Get repair symptoms for the given model no.</info>
	   	<attribute>
			<name>modelNumber</name>
			<type>java.lang.String</type>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
